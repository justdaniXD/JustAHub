function CorrectKeyNotification( ... )
	OrionLib:MakeNotification({
		Name = "Correct Key!",
		Content = "You Have Entered The Correct Key!",
		Image = "rbxassetid://4483345998",
		Time = 5
	})
end

function IncorrectKeyNotification( ... )
	OrionLib:MakeNotification({
		Name = "Incorrect Key!",
		Content = "You Have Entered The Incorrect Key!",
		Image = "rbxassetid://4483345998",
		Time = 5
	})
end

local Tab = Window:MakeTab({
	Name = "Key",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

Tab:AddTextbox({
	Name = "Enter Key!",
	Default = "",
	TextDisappear = true,
	Callback = function(Value)
		_G.KeyInput = Value
	end	  
})

Tab:AddButton({
	Name = "Check Key",
	Callback = function()
      	if _G.KeyInput == _G.Kontol then
		MakeScriptHub()
		CorrectKeyNotification()
		OrionLib:Destroy()
		else
			IncorrectKeyNotification()
		end
  	end    
})

Tab:AddButton({
	Name = "‚ú®Discord Server‚ú®",
	Callback = function()
			OrionLib:MakeNotification({
			Name = "Link Discord Server",
			Content = "Discord Server Link Has Been Copied To Clipboard!",
			Image = "rbxassetid://4483345998",
			Time = 20
		})
    setclipboard("https://discord.gg/XBFwtTbP3A")
  	end 
})

Tab:AddButton({
	Name = "Get Free Key!",
	Callback = function()
			OrionLib:MakeNotification({
			Name = "Link Key!",
			Content = "Key Has Been Copied To Clipboard!",
			Image = "rbxassetid://4483345998",
			Time = 20
		})
    setclipboard("https://discord.gg/XBFwtTbP3A")
  	end    
})

